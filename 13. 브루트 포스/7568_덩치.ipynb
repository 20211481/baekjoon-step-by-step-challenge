      "source": [
        "N=int(input()) # 전체 사람 수\n",
        "size=[] # 사이즈 리스트\n",
        "record=[1]*N #등수\n",
        "checked_person=set() #확인한 사람이 추가될 세트\n",
        "for i in range(N):\n",
        "  a,b=map(int,input().split())\n",
        "  size+=[(a,b)] #리스트에 추가\n",
        "\n"
      ],
      "execution_count": 14,
