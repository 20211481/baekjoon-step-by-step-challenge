      "source": [
        "for nth, base in enumerate(size):\n",
        "  for compare in size:\n",
        "    if base==compare:\n",
        "      continue\n",
        "    if base[0]< compare[0] and base[1]<compare[1]:\n", #키와 몸무게 두가지 다 작을 때\n",
        "        record[nth]+=1\n",
        "for i in record:\n",
        "  print(i, end=\" \")\n",
        "\n",
        "    \n",
        "  "
      ],
